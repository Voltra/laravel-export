{
    "name": "voltra/laravel-export",
    "description": "Create a static site bundle from a Laravel app",
    "keywords": [
        "voltra",
        "spatie",
        "laravel",
        "laravel-export"
    ],
    "replace": {
        "voltra/laravel-export": "<=1.2.1"
    },
    "homepage": "https://github.com/Voltra/laravel-export",
    "license": "MIT",
    "authors": [
        {
            "name": "Voltra",
            "email": "dev@ludwig-guerin.fr",
            "homepage": "https://github.com/Voltra",
            "role": "Developer"
        },
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.8.1",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "nyholm/psr7": "^1.8.1",
        "psr/http-message": "^2.0",
        "spatie/crawler": "^8.1",
        "symfony/console": "^6.4.2|^7.0",
        "symfony/dom-crawler": "^6.0|^7.0",
        "symfony/http-foundation": "^6.4.2|^7.0",
        "symfony/process": "^6.4.2|^7.0",
        "symfony/psr-http-message-bridge": ">=2.3.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.20|^9.0|^10.0",
        "pestphp/pest": "^2.30|^3.7",
        "pestphp/pest-plugin-laravel": "^2|^3.1"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Export\\ExportServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Export\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Export\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    }
}